def narrow(arr):
    #total = 0
    total = sum(arr)
    if total<10:
        print(total)
    else:
        arr = [int(d) for d in str(total)]
        narrow(arr)

        
while True:
    n = input()
    if n == str(0):
        break
    else:
        arr = [int(d) for d in str(n)]
        #print(arr)
        narrow(arr)

        
