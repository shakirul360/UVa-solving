t = int(input())
#x = input() or 0
y = input() or 0
for q in range(t):
    a = int(input())
    f = int(input())
    for u in range(f):
        for i in range(a):
            j = str(i)
            print(j * i)
        for i in range(a,0,-1):
            j = str(i)
            print(j * i)
    if q != (t-1):
        y = input() or 0
